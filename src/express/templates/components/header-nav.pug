-
  const registerText = isAuth ? `Алёна Фролова` : `Регистрация`;
  const enterText = isAuth ? `Выйти` : `Вход с паролем`;

nav.header__nav
  ul.header__nav-list
    li.header__list-item
      a.header__registration(href='/register')= registerText
    li.header__list-item
      a.header__enter(href='/login')= enterText
